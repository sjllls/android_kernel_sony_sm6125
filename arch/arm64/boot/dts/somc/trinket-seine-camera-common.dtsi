/* Flash configuration */
&pmi632_flash0 {
	qcom,duration-ms = <450>;
	qcom,hdrm-voltage-mv = <325>;
};

&pmi632_flash1 {
	qcom,duration-ms = <450>;
	qcom,hdrm-voltage-mv = <325>;
};

&pmi632_torch0 {
	qcom,max-current = <100>;
	qcom,current-ma = <25>;
	qcom,hdrm-voltage-mv = <325>;
};

&pmi632_torch1 {
	qcom,max-current = <100>;
	qcom,current-ma = <25>;
	qcom,hdrm-voltage-mv = <325>;
};

&led_flash0 {
	cell-index = <0>;
	compatible = "qcom,camera-flash";
	qcom,flash-source = <&pmi632_flash0 &pmi632_flash1>;
	qcom,torch-source = <&pmi632_torch0 &pmi632_torch1>;
	qcom,switch-source = <&pmi632_switch0 &pmi632_switch0>;
	status = "ok";
};

&actuator0 {
	status = "disabled";
};

&actuator1 {
	status = "disabled";
};

&eeprom0 {
	status = "disabled";
};

&eeprom1 {
	status = "disabled";
};

&eeprom2 {
	status = "disabled";
};

&cci {
	qcom,camera@0 {
		status = "disabled";
	};

	qcom,camera@1 {
		status = "disabled";
	};

	qcom,camera@2 {
		status = "disabled";
	};

	seine_actuator0: qcom,seine-actuator@0 {
		cell-index = <0>;
		reg = <0x0>;
		compatible = "qcom,actuator";
		qcom,cci-master = <0>;
	};

	seine_actuator1: qcom,seine-actuator@1 {
		cell-index = <1>;
		reg = <0x1>;
		compatible = "qcom,actuator";
		qcom,cci-master = <1>;
	};

	seine_eeprom0: qcom,seine-eeprom@0 {
		cell-index = <0>;
		reg = <0>;
		compatible = "qcom,eeprom";
		cam_vdig-supply = <&S6A>;
		qcom,cam-vreg-name = "cam_vdig";
		qcom,cam-vreg-min-voltage = <1200000>;
		qcom,cam-vreg-max-voltage = <1200000>;
		qcom,cam-vreg-op-mode = <105000>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active
				&cam_sensor_rear_active>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend
				&cam_sensor_rear_suspend>;
		gpios = <&tlmm 34 0>,
			<&tlmm 48 0>,
			<&tlmm 119 0>,
			<&tlmm 117 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vio = <3>;
		qcom,gpio-req-tbl-num = <0 1 2 3>;
		qcom,gpio-req-tbl-flags = <1 0 0 2>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK0",
					"CAM_RESET0", "CAM_VDIG0", "CAM_IO";
		qcom,sensor-position = <0>;
		qcom,cci-master = <0>;
		status = "ok";
		clocks = <&clock_gcc GCC_CAMSS_MCLK0_CLK_SRC>,
				<&clock_gcc GCC_CAMSS_MCLK0_CLK>;
		clock-names = "cam_src_clk", "cam_clk";
		qcom,clock-rates = <24000000 0>;
	};

